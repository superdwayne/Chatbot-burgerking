{
  "version": 2,
  "builds": [
    {
      "src": "server.js",
      "use": "@vercel/node"
    },
    {
      "src": "chat-front/package.json",
      "use": "@vercel/static-build",
      "config": {
        "distDir": "chat-front/build"
      }
    }
  ],
  "routes": [
    { "src": "/api/(.*)", "dest": "/api/$1" },
    { "src": "/(.*)", "dest": "/chat-front/build/index.html" }
  ]
}
